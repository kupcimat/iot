[Unit]
Description=Webthings server service
After=network.target

[Service]
Type=simple
Restart=always
RestartSec=10
User=pi
WorkingDirectory=/home/pi/webthings-server
EnvironmentFile=/home/pi/webthings-server/config/environment
ExecStart=/home/pi/webthings-server/run-server.sh

[Install]
WantedBy=multi-user.target
